#include<iostream>
using namespace std;

int main()
{
    //cout << "Hello,World!\n";
    int r,s=0,x=10;
    int n;
    cin>>n;
    while(n!=0)
    {
        r=n%10;
        s=s+(r*x);
        n=n/10;
        x--;
    }
    if(s%11==0)
    {
        cout<<"Legal ISBN";
    }
    else
    {
        cout<<"Illegal ISBN";
    }
    return 0;
}
