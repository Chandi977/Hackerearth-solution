#include <stdio.h>
#include <string.h>
int main()
{
    char string[100000];
    int i, length;
    scanf("%s", string);
    length = strlen(string);
    for(i=0; i < length ; i++)
    {
        if(string[i] == string[length-i-1])
        {
            printf("YES");
            break;
        }
        else
        {
            printf("NO");
            break;
        }
    }
    return 0;
}
