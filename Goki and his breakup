#include<iostream>
using namespace std;

int main()
{
    int t_person,skill,personSkill[100000];
    cin>>t_person>>skill;
    for(int i=0; i<t_person; i++)
    {
        cin>>personSkill[i];
    }
    for(int i=0; i<t_person; i++)
    {
        if(personSkill[i]>=skill)
            cout<<"YES"<<endl;
        else
            cout<<"NO"<<endl;
    }
    return 0;
}
